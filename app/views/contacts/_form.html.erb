<%= form_for @contact do |f| %>
<%= f.label 'Prénom' %>
<%= f.text_field :first_name %><br><br>
<%= f.label 'Nom' %>
<%= f.text_field :last_name %><br><br>
<%= f.label 'Fonction' %>
<%= f.text_field :function %><br><br>
<%= f.label 'N°tél' %>
<%= f.text_field :phone %><br><br>
<%= f.label 'Email' %>
<%= f.email_field :email %><br><br>
<%= f.label 'Mot de passe' %>
<%= f.password_field :password %><br><br>
<%= f.submit 'Envoyer' %>
<% end %>